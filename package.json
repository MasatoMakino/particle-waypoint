    "typedocs": "npx typedoc --out ./docs/api src/index.ts",
    "watch:tsc": "npx tsc -W --incremental --project tsconfig.json & npx tsc -W --incremental --project tsconfig.esm.json"
  },
  "dependencies": {
    "@masatomakino/raf-ticker": "0.2.9"
  },
  "bugs": {
