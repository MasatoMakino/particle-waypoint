{
  "name": "particle-waypoint",
  "version": "0.5.2",
  "description": "Path module for calculating waypoints.",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "module": "./esm/index.js",
  "files": [
    "lib",
    "esm"
  ],
  "repository": "https://MasatoMakino@github.com/MasatoMakino/particle-waypoint.git",
  "author": "MasatoMakino <unagiinu.g@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^28.1.1",
    "eventemitter3": "^4.0.7",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.0",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^28.0.4",
    "typescript": "^4.7.3"
  },
  "scripts": {
    "prepare": "husky install",
    "test": "jest --coverage",
    "start:dev": "npm run server & npm run watch:tsc & npm run watch:demo",
    "build": "npm run buildTS && npm run typedocs",
    "buildTS": "npx tsc --project tsconfig.cjs.json & npx tsc --project tsconfig.esm.json",
    "typedocs": "npx typedoc --out ./docs/api src/index.ts",
    "watch:tsc": "npx tsc -W --incremental --project tsconfig.json & npx tsc -W --incremental --project tsconfig.esm.json"
  },
  "dependencies": {
    "raf-ticker": "https://github.com/MasatoMakino/raf-ticker.git"
  }
}
